{
	"name": "template-node",
	"version": "0.0.0",
	"private": true,
	"description": "Node + PlantUML Starter Template",
	"license": "MIT",
	"scripts": {
		"start": "node ./bin/www",
		"dev": "nodemon ./bin/www",
		"eslint:check": "npx eslint .",
		"eslint:fix": "npx eslint --fix .",
		"test": "DOTENV_CONFIG_PATH=./.env.test.local jest --detectOpenHandles --forceExit",
		"test:watch": "DOTENV_CONFIG_PATH=./.env.test.local jest --watch --coverage --detectOpenHandles --forceExit",
		"test:coverage": "DOTENV_CONFIG_PATH=./.env.test.local jest --coverage --detectOpenHandles --forceExit"
	},
	"devDependencies": {
		"eslint": "^8.19.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"jest": "^29.6.2",
		"nodemon": "^2.0.19",
		"prettier": "^2.7.1",
		"supertest": "^6.3.3"
	},
	"dependencies": {
		"axios": "^1.4.0",
		"chalk": "^4.1.2",
		"cookie-parser": "^1.4.6",
		"debug": "^4.3.4",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"http-errors": "^2.0.0",
		"mongoose": "^7.4.1",
		"mongoose-autopopulate": "^1.0.1",
		"morgan": "^1.10.0",
		"pug": "^3.0.2"
	},
	"jest": {
		"setupFiles": [
			"dotenv/config"
		],
		"setupFilesAfterEnv": [
			"./tests/setup.js"
		]
	}
}
